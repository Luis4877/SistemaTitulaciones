        <servlet-name>MultipleUpload</servlet-name>
        <url-pattern>/MultipleUpload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControlCert</servlet-name>
        <url-pattern>/ControlCert</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SERVDiplomado</servlet-name>
