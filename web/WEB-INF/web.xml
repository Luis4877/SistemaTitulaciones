        <servlet-class>sistema.controller.ControlDiplo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>controlAdmin</servlet-name>
        <url-pattern>/controlAdmin</url-pattern>
        <!-- <url-pattern>/controlAdmin</url-pattern>  -->
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>controlProm</servlet-name>
        <url-pattern>/controlProm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UploadServlet</servlet-name>
        <url-pattern>/UploadServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SERVSolicitud</servlet-name>
        <url-pattern>/SERVSolicitud</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReportsController</servlet-name>
        <url-pattern>/ReportsController</url-pattern>
    </servlet-mapping>
