    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControlDiplo</servlet-name>
        <url-pattern>/ControlDiplo</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
